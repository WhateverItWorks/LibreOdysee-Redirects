{
    "name": "LibreOdysee Redirects",
    "version": "1.2",
    "description": "Redirects Odysee links to LibreOdysee",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://open.lbry.com/*",
        "*://odysee.com/*",
        "<all_urls>"
    ],
    "icons": {
        "48": "img/logo-48.png",
        "96": "img/logo-96.png",
        "128": "img/logo-128.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "manifest_version": 2
}
